{
  "DatabaseSettings": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "BasketDb",
    "CollectionName": "ShoppingCart"
  },
  "GrpcSettings": {
    "DiscountUrl": "https://localhost:6062"
  },
  "Observability": {
    "ServiceName": "Basket.Api",
    "ServiceVersion": "1.0.0",
    "EnableTracing": true,
    "EnableMetrics": true,
    "EnableLogExporter": false,
    "EnableSerilogConsole": true,
    "EnableAspNetCoreInstrumentation": true,
    "EnableHttpClientInstrumentation": true
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
