    },
    {
      "label": "build-webapp",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/src/Uis/WebApp/WebApp.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile",
      "group": "build"
    },
    {
      "label": "build-all-services",
      "dependsOrder": "parallel",
      "dependsOn": [
        "build-merchant-api",
        "build-catalog-api",
        "build-basket-api",
        "build-order-api"
      ],
      "group": "build"
    },
    {
      "label": "build-backend-services",
      "dependsOrder": "parallel",
      "dependsOn": [
        "build-merchant-api",
        "build-catalog-api",
        "build-basket-api",
        "build-order-api",
        "build-bff-api"
      ],
      "group": "build"
    },
    {
      "label": "build-all-projects",
      "dependsOrder": "parallel",
      "dependsOn": [
        "build-merchant-api",
        "build-catalog-api",
        "build-basket-api",
        "build-order-api",
        "build-bff-api",
        "build-webapp"
      ],
      "group": "build"
    },
    {
      "label": "build-solution",
      "command": "dotnet",
      "type": "process",
      "args": [
        "build",
        "${workspaceFolder}/MarketSpace.sln",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "problemMatcher": "$msCompile",
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "clean-solution",
      "command": "dotnet",
      "type": "process",
      "args": [
        "clean",
        "${workspaceFolder}/MarketSpace.sln"
      ],
      "problemMatcher": "$msCompile",
      "group": "build"
    },
    {
      "label": "restore-solution",
      "command": "dotnet",
      "type": "process",
      "args": [
        "restore",
        "${workspaceFolder}/MarketSpace.sln"
      ],
      "problemMatcher": "$msCompile",
      "group": "build"
    },
    {
      "label": "watch-merchant-api",
      "command": "dotnet",
      "type": "process",
      "args": [
        "watch",
        "run",
        "--project",
        "${workspaceFolder}/src/Services/Merchant/Merchant.Api/Merchant.Api.csproj"
      ],
      "options": {
        "cwd": "${workspaceFolder}/src/Services/Merchant/Merchant.Api",
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development",
          "ASPNETCORE_URLS": "https://localhost:5055;http://localhost:5005"
        }
      },
      "isBackground": true,
      "problemMatcher": [
        {
          "pattern": [
            {
              "regexp": "\\G",
              "file": 1,
              "location": 2,
              "message": 3
            }
          ],
          "background": {
            "activeOnStart": true,
            "beginsPattern": "^\\s*Attempting to restart process",
            "endsPattern": "^\\s*Application started\\. Press Ctrl\\+C to shut down\\."
          }
        }
      ],
      "group": "build"
    },
